{
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node22.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist/client"
}